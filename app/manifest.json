{
  "name": "Schengen Area",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/logo/16.png",
    "128": "images/logo/128.png"
  },
  "browser_action": {
    "default_icon": "images/logo/128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "identity",
    "storage"
  ],
  "offline_enabled": true
}
